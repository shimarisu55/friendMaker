<%= form_with(model: profile, local: true) do |form| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.number_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :profile_q_number %>
    <%= form.number_field :profile_q_number %>
  </div>

  <div class="field">
    <%= form.label :profile_a %>
    <%= form.text_field :profile_a %>
  </div>

  <div class="field">
    <%= form.label :delete_flag %>
    <%= form.check_box :delete_flag %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
