

<h1>最近質問に答えたユーザー</h1>



<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Animal</th>
      <th>Color</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.icon_animal %></td>
        <td><%= user.icon_color %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to '編集', edit_user_path(user) %></td>
        <td><%= link_to '退会する', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

